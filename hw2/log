rm -f spmm_csr_driver spmm_csr_driver.o mm_helper.o spmm_csc_driver spmm_csc_driver.o mm_helper.o spmm_opt_driver spmm_opt_driver.o mm_helper.o 
nvcc  --gpu-architecture=compute_60 --gpu-code=sm_60 -O3 -c spmm_csr_driver.cu -o spmm_csr_driver.o
gcc -O3 -c mm_helper.cpp -o mm_helper.o
nvcc --gpu-architecture=compute_60 --gpu-code=sm_60 -O3 spmm_csr_driver.o mm_helper.o -o spmm_csr_driver
Change line the above line in Makefile to "all: $(EXECSR) $(EXECSC)" to build both CSR and CSC versions
Change line the above line in Makefile to "all: $(EXECSR) $(EXECSC) $(EXEOPT)" to build both CSR, CSC and OPT versions
